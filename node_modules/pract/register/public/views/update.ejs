<%- include('header') %>

<div class="agileits-main">
		<div class="wrap">
		<form action="/update1" method="post">
			<ul>
				<li class="text">Enter Id  :  </li>
				<li><input name="txtid" type="text" required></li>
			</ul>
				   <% users.forEach(function(users) { %>
									<%= users.fname%>

				<input type="text" name="fname"  value=<%= users.fname%> required /> <br />
				<input type="text" name="lname"  value=<%= users.lname%> required /> <br />
					
				<input type="text" name="email" id="emailid" onblur="return validatEmail();" placeholder="email Id" required  /> 
				<span id="errormail"><font color="white">&nbsp;</font></span>

				<input type="text" name="epass" id="pass" onblur="return validatepass();" placeholder="enter password" required  /> 
				
				<span id="errorpass"><font color="white">&nbsp;</font></span>

				

								
						 <% }); %>         
					
			
			
			
			<div class="clear"></div>
			<div class="agile-submit">
				<input type="submit" value="submit">
				
			</div>
			</form>
		</div>	
	</div>
	<%- include('footer') %>