<div class="navbar-header">   
		<div class="clearfix clear"></div>    <br />
			

			<% if(typeof users != 'undefined' && users.length > 0) { %>		 
						<ul class="nav navbar-nav">
						   <% users.forEach(function(user) { %>
							<li>
							    <p><%= user.uid %>
								<br />
								<p><%= user.uname %>
								<br />
								<p><%= user.image %>
								<br />
								
							</li>
						 <% }); %>         
						</ul>
			<% } %> 

		</div>
</div>
