<%- include('header') %>
<script src="/views/valid.js"></script>

<div class="agileits-main">
		<div class="wrap">
		
		<form action="/insert" name='registration'  onsubmit="return formValidation();" method="post">
			<ul>
				<li class="text">FirstName  :  </li>
				<li><input type="text" name="fname"  placeholder="Enter First Name" required /> </li>
			</ul>
			<ul>
				<li class="text">LastName  :  </li>
				<li><input type="text" name="lname"  placeholder="Enter a Last Name" required /> </li>
			</ul>
			
			<ul>
				<li class="text">E-mail  :  </li>
		     
		     <li>  <input type="text" name="email" id="emailid" onblur="return validateEmail();" placeholder="Enter e-mail id" required /> </li>
				<li><span id="emailerror"></span></li>
			</ul>


			<ul>
				<li class="text">Password  :  </li>
				<li><input name="pass" type="text" id="passid" onblur="passid_validation();" placeholder="Enter Password" required></li>
				<li><span id="passerror"></span></li>
			</ul>
			
			
			<div class="clear"></div>
			
				<input type="submit" value="submit">
				
			</div>
			</form>
		</div>	
	</div>
	<%- include('footer'); %>